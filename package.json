{
  "name": "electron-auto-update-example",
  "version": "1.0.3",
  "description": "",
  "main": "main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/hemanthkishore/autoupdatetest.git"
  },
  "scripts": {
    "start": "electron .",
    "build": "electron-builder build --mac --publish never",
    "deploy": "electron-builder build --mac --publish always",
    "postinstall": "electron-builder install-app-deps",
    "pack": "electron-builder --dir",
    "dist": "electron-builder -mw"
  },
  "build": {
    "appId": "com.amy.mydrreddys",
    "productName": "Amy",
    "copyright": "Copyright Â© Bitonic Technology Labs, Inc. Silverside road, Wilmington, Delaware 02139.",
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "build/amy.ico"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": false,
      "runAfterFinish": true
    },
    "mac": {
      "target": [
        "dmg",
        "zip"
      ],
      "category": "public.app-category.utilities",
      "icon": "build/amy.icns"
    },
    "dmg": {
      "title": "AMY"
    }
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "electron": "^18.3.7",
    "electron-builder": "^20.40.2"
  },
  "dependencies": {
    "electron-updater": "^4.0.6"
  }
}